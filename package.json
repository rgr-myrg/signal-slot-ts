{
  "name": "signal-ts",
  "version": "1.2.3",
  "description": "Simple to use Signal pattern written in Typescript. Signals are typed.",
  "main": "dist/Signal.js",
  "types": "dist/Signal.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jasmine-ts \"src/test/*.spec.ts\"",
    "coverage": "nyc -e .ts -x \"./src/test/*.ts\" jasmine-ts \"./src/test/*.ts\"",
    "coverage-text": "nyc -e .ts report --reporter=text-lcov > coverage/coverage.lcov npm test",
    "nyc-report": "nyc -e .ts report --reporter=json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rgr-myrg/signal-ts.git"
  },
  "keywords": [
    "typescript",
    "signal",
    "event",
    "emitter"
  ],
  "author": "<solidity@gmx.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rgr-myrg/signal-ts/issues"
  },
  "homepage": "https://github.com/rgr-myrg/signal-ts#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/jasmine": "^2.8.8",
    "codecov": "^3.0.0",
    "istanbul": "^0.4.5",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.2.1",
    "nyc": "^11.5.0",
    "ts-node": "^4.1.0",
    "typescript": "^3.0.3"
  }
}
